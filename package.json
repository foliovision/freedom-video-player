{
  "name": "flowplayer",
  "version": "7.2.7",
  "description": "The HTML5 video player for the web",
  "main": "dist/flowplayer.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mochify ./test/unit/*.js",
    "test-watch": "mochify -w ./test/unit/*.js",
    "fixmeprepublish": "make clean && make skin && make min && make flash && make",
    "testBundle": "browserify -o dist/test-bundle.js test/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/flowplayer/flowplayer.git"
  },
  "author": "",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/flowplayer/flowplayer/issues"
  },
  "jshintConfig": {
    "browser": true,
    "node": true,
    "strict": true,
    "newcap": false,
    "multistr": true
  },
  "dependencies": {
    "bean": "^1.0.15",
    "class-list": "~0.1.1",
    "computed-style": "^0.3.0",
    "es5-shim": "^4.6.2",
    "extend-object": "~1.0.0",
    "is-function": "~1.0.2",
    "scriptjs": "^2.5.9"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "brfs": "^2.0.2",
    "browserify": "^17.0.0",
    "browserify-derequire": "^1.1.1",
    "ie8": "=0.8.1",
    "mochify": "~8.1.0",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.1",
    "postcss-unopacity": "^2.0.0",
    "uglify-js": "^3.14.2"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-unopacity": {
        "method": "copy"
      }
    }
  }
}
